{
	"name": "ProtectEntireWiki",
	"author": "LBLaiSiNanHai",
	"description": "This extension can protect entire wiki",
	"version": "indev",
	"license-name": "BSD-3-Clause",
	"type": "antispam",
	"manifest_version": 2,
	"AutoloadedClasses": {
		"ProtectEntireWiki": "includes/ProtectEntireWiki.php",
		"PEWEditingUIHooksRw": "includes/PEWEditingUIHooksRw.php",
		"PEWEditingSavingHooks": "includes/PEWEditingSavingHooks.php",
		"PEWRollbackingHooks": "includes/PEWRollbackingHooks.php"
	},
	"HookHandlers": {
		"PEWEditingUIHooksRwHookHandler": {
			"class": "PEWEditingUIHooksRw",
			"services": []
		},
		"PEWEditingSavingHooksHookHandler": {
			"class": "PEWEditingSavingHooks",
			"services": []
		},
		"PEWRollbackingHooksHookHandler": {
			"class": "PEWRollbackingHooks",
			"services": []
		}
	},
	"Hooks": {
		"EditPage::showEditForm:initial": "PEWEditingUIHooksRwHookHandler",
		"EditPage::attemptSave": "PEWEditingSavingHooksHookHandler",
		"RollbackComplete": "PEWRollbackingHooksHookHandler"
	},
	"MessagesDirs": {
		"ProtectEntireWiki": [
			"i18n"
		]
	}
}